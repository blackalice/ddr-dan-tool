{
  "name": "ddr-toolkit",
  "main": "./_worker.js",
  "compatibility_date": "2025-06-05",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "directory": "./dist",
    "binding": "ASSETS",
    "not_found_handling": "single-page-application"
  },
  "build": {
    "command": "npm run build"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "users",
      "database_id": "users-db"
    }
  ]
}
