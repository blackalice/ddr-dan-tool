{
  "name": "ddr-toolkit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:prepare\" \"npm:dev:wrangler\"",
    "dev:prepare": "node scripts/generate-sm-list.mjs && node scripts/generate-processed-data.mjs && node scripts/generate-song-meta.mjs && npm run build:audio && node scripts/generate-radar.mjs && vite",
    "dev:wrangler": "wrangler dev",
    "build": "vite build",
    "prebuild": "node scripts/generate-sm-list.mjs && node scripts/generate-processed-data.mjs && node scripts/generate-song-meta.mjs && npm run build:audio && node scripts/generate-radar.mjs",
    "postinstall": "node scripts/generate-sm-list.mjs && node scripts/generate-processed-data.mjs && node scripts/generate-song-meta.mjs && npm run build:audio && node scripts/generate-radar.mjs",
    "lint": "eslint .",
    "preview": "vite preview",
    "cf:dev": "wrangler dev",
    "deploy": "npm run build && npm run deploy:worker && npm run deploy:pages",
    "deploy:worker": "wrangler deploy --env production",
    "deploy:pages": "wrangler pages deploy dist",
    "parse:scores": "node scripts/parse-ganymede-html.mjs",
    "build:audio": "node scripts/build-audio-lengths.mjs ddrbits"
  },
  "dependencies": {
    "@dnd-kit/core": " ^6.1.0 ",
    "@dnd-kit/sortable": " ^8.0.0 ",
    "@dnd-kit/utilities": " ^3.2.2 ",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@google/generative-ai": "^0.24.1",
    "argon2-browser": "^1.18.0",
    "chart.js": "^4.5.0",
    "cheerio": "^1.1.2",
    "clsx": "^2.1.1",
    "fraction.js": "^4.0.13",
    "hono": "^4.8.5",
    "jose": "^6.0.12",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.3",
    "react-select": "^5.10.1",
    "react-window": "^1.8.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "concurrently": "^9.2.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "music-metadata": "^11.8.3",
    "vite": "^7.0.0",
    "wrangler": "^4.36.0"
  }
}
